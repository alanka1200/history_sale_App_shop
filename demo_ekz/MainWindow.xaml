<Window x:Class="demo_ekz.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:demo_ekz"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>


        <Grid.RowDefinitions>
            <RowDefinition Height="50*"/>
            <RowDefinition Height="361*"/>
            <RowDefinition Height="23*"/>
        </Grid.RowDefinitions>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Margin="0,50,0,0" Grid.RowSpan="2"/>


        <TextBlock Grid.Row="0" Background="AliceBlue" Text="Beauty Salon" FontSize="30" HorizontalAlignment="Center" Width="172"/>
        <Image x:Name="Logo" Source="/Resources/beauty_logo.png" HorizontalAlignment="Left" Margin="5,5,0,5" Grid.Row="0" Width="40"/>
        <Grid Background="#bae3e8" Grid.Row="0" ZIndex="-2"></Grid>
    </Grid>
</Window>
